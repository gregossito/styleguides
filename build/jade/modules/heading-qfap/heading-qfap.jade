mixin heading-qfap(data)
  .heading-qfap
    if (data.logo)
      a.heading-qfap-logo(href=data.logo.href)
        include ../../../assets/images/modules/heading-qfap/logo-qfap.svg
        .heading-qfap-logo-text= data.logo.text
    .heading-qfap-title
      p= data.text
      if (data.link)
        p.heading-qfap-title-link
          span.icon-link-external
          a(href=data.link.href)= data.link.text
