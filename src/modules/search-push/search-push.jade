include ../button/button

mixin search-push(data)
  .search-push
    .search-push-text= data.text
    - var button = JSON.parse(JSON.stringify(data.button)) // clone the data object
    - button.modifiers = ["action"] // overwrite the modifiers
    - button.attributes = {"data-action": "open-search", "aria-controls": "rheader-search"}
    +button(button)
